{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "baseUrl": "src",
    "paths": {
      "@components/*": ["components/*"],
      "@styled/*": ["components/styled/*"],
      "@pages/*": ["components/pages/*"],
      "@assets/*": ["assets/*"],
      "@services/*": ["services/*"],
      "@utils/*": ["utils/*"],
      "@config/*": ["config/*"],
      "@storage/*": ["storage/*"]
    }
  },
  "include": ["src"]
}